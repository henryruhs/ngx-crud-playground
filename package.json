{
	"name": "ngx-crud-playground",
	"description": "Playground for ngx-crud",
	"version": "1.0.0",
	"homepage": "https://playground.ngx-crud.com",
	"license": "MIT",
	"keywords":
	[
		"angular",
		"http",
		"crud",
		"aborting",
		"caching",
		"observing"
	],
	"author":
	{
		"name": "Henry Ruhs",
		"url": "https://henryruhs.com"
	},
	"bugs":
	{
		"url": "https://github.com/henryruhs/ngx-crud-playground/issues"
	},
	"repository":
	{
		"type": "git",
		"url": "https://github.com/henryruhs/ngx-crud-playground.git"
	},
	"dependencies":
	{
		"@angular/animations": "14.1.0",
		"@angular/common": "14.1.0",
		"@angular/compiler": "14.1.0",
		"@angular/core": "14.1.0",
		"@angular/forms": "14.1.0",
		"@angular/platform-browser": "14.1.0",
		"@angular/platform-browser-dynamic": "14.1.0",
		"@angular/router": "14.1.0",
		"harmonize.css": "4.1.4",
		"ngx-crud": "12.0.0-beta.1",
		"rxjs": "7.5.6",
		"rxjs-collection": "1.0.0",
		"tslib": "2.4.0",
		"zone.js": "0.11.7"
	},
	"devDependencies":
	{
		"@angular-builders/jest": "14.0.0",
		"@angular-devkit/build-angular": "14.1.0",
		"@angular/cli": "14.1.0",
		"@angular/compiler-cli": "14.1.0",
		"@cypress/schematic": "2.0.0",
		"@krema/angular-eslint-stylelint-builder": "1.2.3",
		"@isnotdefined/eslint-config": "6.1.2",
		"@isnotdefined/stylelint-config": "3.5.0",
		"@types/jest": "28.1.6",
		"@types/node": "18.6.4",
		"cypress": "10.4.0",
		"cypress-terminal-report": "4.1.1",
		"cross-env": "7.0.3",
		"eslint": "8.21.0",
		"jest": "28.1.3",
		"jest-preset-angular": "12.2.0",
		"postcss-scss": "4.0.4",
		"stylelint": "14.9.1",
		"typescript": "4.7.4",
		"wait-on": "6.0.1"
	},
	"scripts":
	{
		"start": "ng serve",
		"start:local": "npm run start -- --configuration=local",
		"start:prod": "npm run start -- --configuration=prod",
		"build": "ng build",
		"build:local": "npm run build -- --configuration=local",
		"build:prod": "npm run build -- --configuration=prod",
		"lint": "ng lint",
		"fix": "npm run lint -- --fix",
		"test": "ng test",
		"e2e": "ng e2e",
		"e2e:local": "cross-env-shell CYPRESS_APP_ENV=local npm run e2e",
		"e2e:prod": "cross-env-shell CYPRESS_APP_ENV=prod npm run e2e",
		"wait-for-serve": "wait-on tcp:4200 && sleep 30"
	}
}
