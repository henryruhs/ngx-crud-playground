		"url": "https://github.com/henryruhs/ngx-crud-playground.git"
	},
	"dependencies":
	{
		"@angular/animations": "15.1.0",
		"@angular/common": "15.1.0",
		"@angular/compiler": "15.1.0",
		"@angular/core": "15.1.0",
		"@angular/forms": "15.1.0",
		"@angular/platform-browser": "15.1.0",
		"@angular/platform-browser-dynamic": "15.1.0",
		"@angular/router": "15.1.0",
		"harmonize.css": "4.1.4",
		"ngx-crud": "13.0.1",
		"rxjs": "7.8.0",
		"rxjs-collection": "2.1.2",
		"tslib": "2.4.1",
		"zone.js": "0.12.0"
	},
	"devDependencies":
	{
		"@angular-builders/jest": "15.0.0",
		"@angular-devkit/build-angular": "15.1.1",
		"@angular/cli": "15.1.1",
		"@angular/compiler-cli": "15.1.0",
		"@cypress/schematic": "2.4.0",
		"@krema/angular-eslint-stylelint-builder": "1.3.0",
		"@isnotdefined/eslint-config": "8.1.0",
		"@isnotdefined/stylelint-config": "4.3.0",
		"@types/jest": "29.2.5",
		"@types/node": "18.11.18",
		"cypress": "12.3.0",
		"cypress-terminal-report": "5.0.0",
		"cross-env": "7.0.3",
		"eslint": "8.32.0",
		"jest": "29.3.1",
		"jest-preset-angular": "12.2.4",
		"postcss-scss": "4.0.6",
		"stylelint": "14.16.1",
		"typescript": "4.8.4",
		"wait-on": "7.0.1"
	},
	"scripts":
	{
		"start": "ng serve",
		"start:local": "npm run start -- --configuration=local",
		"start:prod": "npm run start -- --configuration=prod",
		"build": "ng build",
		"build:local": "npm run build -- --configuration=local",
		"build:prod": "npm run build -- --configuration=prod",
		"lint": "ng lint",
		"fix": "npm run lint -- --fix",
		"test": "ng test",
		"e2e": "ng e2e",
		"e2e:local": "cross-env-shell CYPRESS_APP_ENV=local npm run e2e",
		"e2e:prod": "cross-env-shell CYPRESS_APP_ENV=prod npm run e2e",
		"wait-for-serve": "wait-on tcp:4200 && sleep 30"
	}
}
