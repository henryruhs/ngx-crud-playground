{
	"name": "ngx-crud-playground",
	"description": "Playground for ngx-crud",
	"version": "1.0.0",
	"homepage": "https://playground.ngx-crud.com",
	"license": "MIT",
	"keywords":
	[
		"angular",
		"http",
		"crud",
		"aborting",
		"caching",
		"observing"
	],
	"author":
	{
		"name": "Henry Ruhs",
		"url": "https://henryruhs.com"
	},
	"bugs":
	{
		"url": "https://github.com/henryruhs/ngx-crud-playground/issues"
	},
	"repository":
	{
		"type": "git",
		"url": "https://github.com/henryruhs/ngx-crud-playground.git"
	},
	"dependencies":
	{
		"@angular/animations": "14.0.0-next.14",
		"@angular/common": "14.0.0-next.14",
		"@angular/compiler": "14.0.0-next.14",
		"@angular/core": "14.0.0-next.14",
		"@angular/forms": "14.0.0-next.14",
		"@angular/platform-browser": "14.0.0-next.14",
		"@angular/platform-browser-dynamic": "14.0.0-next.14",
		"@angular/router": "14.0.0-next.14",
		"harmonize.css": "4.1.4",
		"ngx-crud": "11.0.0-next.2",
		"rxjs": "7.5.5",
		"tslib": "2.4.0",
		"zone.js": "0.11.5"
	},
	"devDependencies":
	{
		"@angular-devkit/build-angular": "14.0.0-next.11",
		"@angular-eslint/builder": "13.2.1",
		"@angular-eslint/eslint-plugin": "13.2.1",
		"@angular-eslint/eslint-plugin-template": "13.2.1",
		"@angular-eslint/template-parser": "13.2.1",
		"@angular/cli": "14.0.0-next.11",
		"@angular/compiler-cli": "14.0.0-next.14",
		"@angular/language-service": "14.0.0-next.14",
		"@cypress/schematic": "1.6.0",
		"@isnotdefined/eslint-config": "4.7.0",
		"@isnotdefined/stylelint-config": "3.2.0",
		"@types/jasmine": "4.0.3",
		"@types/jasminewd2": "2.0.10",
		"@types/node": "17.0.25",
		"cypress": "9.5.4",
		"cross-env": "7.0.3",
		"eslint": "8.14.0",
		"jasmine-core": "4.1.0",
		"jasmine-spec-reporter": "7.0.0",
		"karma": "6.3.19",
		"karma-chrome-launcher": "3.1.1",
		"karma-jasmine": "5.0.0",
		"ncss-linter": "6.2.0",
		"postcss-scss": "4.0.3",
		"stylelint": "14.7.1",
		"ts-loader": "9.2.8",
		"ts-node": "10.7.0",
		"typescript": "4.6.3",
		"wait-on": "6.0.1"
	},
	"scripts":
	{
		"start": "ng serve",
		"start:local": "npm run start -- --configuration=local",
		"start:prod": "npm run start -- --configuration=prod",
		"build": "ng build",
		"build:local": "npm run build -- --configuration=local",
		"build:prod": "npm run build -- --configuration=prod",
		"lint": "ng lint && stylelint ./src/**/**/**/*.scss && ncss-linter",
		"fix": "ng lint --fix && stylelint ./src/**/**/**/*.scss --fix",
		"test": "ng test",
		"e2e": "ng e2e",
		"e2e:local": "cross-env-shell CYPRESS_APP_ENV=local npm run e2e",
		"e2e:prod": "cross-env-shell CYPRESS_APP_ENV=prod npm run e2e",
		"wait-for-serve": "wait-on tcp:4200 && sleep 60"
	}
}
